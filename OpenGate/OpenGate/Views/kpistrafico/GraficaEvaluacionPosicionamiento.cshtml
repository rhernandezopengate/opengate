<div id="chartEvalPosicionamiento"></div>

<script type="text/javascript">
    var chartPosicionamiento = new CanvasJS.Chart("chartEvalPosicionamiento",{
        animationEnabled: true,
        title: {
            text: "Porcentaje del Mes: " + "@ViewBag.MesEvalOcupacion"
        },
        data: [{
            type: "doughnut",
            startAngle: 90,
            indexLabelFontSize: 10,
            indexLabel: "{label}-{y}%",
            toolTipContent: "<b>{label}:</b> {y}%",
            dataPoints: @Html.Raw(ViewBag.DataPointPosicionamiento),
        }]
    });
    chartPosicionamiento.render();
</script>