@using OpenGate.Entidades
@model List<kpistrafico>

<table id="tblOnTimePosicionamiento" class="table table-bordered" style="cursor:pointer;">
    <thead>
        <tr>
            <th>Mes</th>
            <th>
                Fletes
            </th>
            <th>
                Ok
            </th>
            <th>
                Desv
            </th>
            <th>
                %
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td style="width:10px">@item.mescarga</td>
            <td style="width:10px">@item.Total</td>
            <td>
                @item.TotalOks
            </td>
            <td>
                @item.TotalDesv
            </td>
            <td>
                @item.Porcentaje%
            </td>
        </tr>
        }

        <tr>
            <td>Total</td>
            <td>
                @ViewBag.TotalEvaluacionPosicionamiento
            </td>
            <td>
               @ViewBag.TotalOkEvaluacionPosicionamiento
            </td>
            <td>
                @ViewBag.TotalDesvEvaluacionPosicionamiento
            </td>
            <td>
                @ViewBag.PorcentajeOkEvaluacionPosicionamiento%
            </td>
        </tr>
    </tbody>
</table>

